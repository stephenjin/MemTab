{
  "manifest_version": 2,

  "name": "MemTab",
  "description": "This extensions replaces the newtab page with a page displaying notecards.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },

  "chrome_url_overrides" : {
    "newtab": "page.html"
  },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}